<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Version>1.0.2-beta.0</Version>
    <Description>Basic reading and writing of CSV (comma-separated value) text.</Description>
    <AssemblyName>Fmbm.Csv</AssemblyName>
    <AssemblyTitle>Fmbm.Csv</AssemblyTitle>
    <Authors>John Donnellan</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageReadmeFile>ReadMe.md</PackageReadmeFile>
    <PackageTags>read;write;csv;basic;C#;F#;fmbm</PackageTags>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/codybartfast/csv</RepositoryUrl>

    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <WarningsAsErrors>Nullable</WarningsAsErrors>
  </PropertyGroup>

  <ItemGroup>
    <None Include="../../ReadMe.md" Pack="true" PackagePath="\" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="fsharp.core" Version="6.0" />
  </ItemGroup>

</Project>
